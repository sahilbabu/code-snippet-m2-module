<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.additional">
            <block
                class="Magento\Framework\View\Element\Template"
                name="magento.framework.view.element.template"
                template="Egooty_SeoToolkits::html/header/hreflang-links.phtml">
                <arguments>
                    <argument name="priority" xsi:type="string">10</argument>
                    <argument name="hreflang_data" xsi:type="object">
                        Egooty\SeoToolkits\ViewModel\Html\Header\HreflangData
                    </argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>